<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="Mosaddek">
    <meta name="keyword" content="FlatLab, Dashboard, Bootstrap, Admin, Template, Theme, Responsive, Fluid, Retina">
    <link rel="shortcut icon" href="~/OuterToolsTemplate/img/favicon.png">

    <title>BiletCini</title>

    <!-- Bootstrap core CSS -->
    <link href="~/OuterToolsTemplate/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/OuterToolsTemplate/css/bootstrap-reset.css" rel="stylesheet">
    <!--external css-->
    <link href="~/OuterToolsTemplate/assets/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link href="~/OuterToolsTemplate/assets/jquery-ui/jquery-ui-1.10.1.custom.min.css" rel="stylesheet" />
    <!-- Custom styles for this template -->
    <link href="~/OuterToolsTemplate/css/style.css" rel="stylesheet">
    <link href="~/OuterToolsTemplate/css/style-responsive.css" rel="stylesheet" />
    <a href="~/lib/jquery/dist/jquery.min.map"></a>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 tooltipss and media queries -->
    <!--[if lt IE 9]>
      <script src="js/html5shiv.js"></script>
      <script src="js/respond.min.js"></script>
    <![endif]-->
</head>

<body>

    <section id="container" class="">
        <!--header start-->
        <header class="header white-bg">
         
           
            <!--logo start-->
            <a asp-action="Index" asp-controller="Home" class="logo">Bilet<span>Cini</span></a>
            <!--logo end-->
            <div class="nav notify-row" id="top_menu">
                <!--  notification start -->
             
            </div>
            <div class="top-nav ">
                <ul class="nav pull-right top-menu">
                    <li>
                        <input type="text" class="form-control search" placeholder="Search">
                    </li>
                    <!-- user login dropdown start-->
                    <li class="dropdown">
                        <a data-toggle="dropdown" class="dropdown-toggle" href="#">
                            <img alt="" src="img/avatar1_small.jpg">
                            <span class="username">@(User.Identity.Name ?? "Kullanıcı")</span>
                            <b class="caret"></b>
                        </a>
                        <ul class="dropdown-menu extended logout">
                            <div class="log-arrow-up"></div>
                            <li><a href="#"><i class=" icon-suitcase"></i>Profile</a></li>
                            <li><a href="#"><i class="icon-cog"></i> Settings</a></li>
                            <li><a href="#"><i class="icon-bell-alt"></i> Notification</a></li>
                            @if (User.Identity.Name != null)
                            {
                                <li><a asp-action="LogOut" asp-controller="Home"> <i class="icon-key"></i> Log Out</a></li>
                            }
                            else
                            {
                                <li><a asp-action="SignIn" asp-controller="Home"> <i class="icon-key"></i> Sign in</a></li>
                            }
                        </ul>
                    </li>
                    <!-- user login dropdown end -->
                </ul>

            </div>
        </header>
        <!--header end-->
        <!--sidebar start-->
       
        <!--sidebar end-->
        <!--main content start-->
        
            <section class="wrapper">
                @RenderBody()

                <!-- page start-->
               
                <!-- page end-->
            </section>
       
        <!--main content end-->
        <!--footer start-->
        <footer class="site-footer">
            <div class="text-center">
                2024 &copy; BiletCini by VectorLab.
                <a href="#" class="go-top">
                    <i class="icon-angle-up"></i>
                </a>
            </div>
        </footer>
        <!--footer end-->


    </section>

    <!-- js placed at the end of the document so the pages load faster -->
    <script src="/OuterToolsTemplate/js/jquery.js"></script>
    <script src="/OuterToolsTemplate/js/bootstrap.min.js"></script>
    <script class="include" type="text/javascript" src="/OuterToolsTemplate/js/jquery.dcjqaccordion.2.7.js"></script>
    <script src="/OuterToolsTemplate/js/jquery.scrollTo.min.js"></script>
    <script src="/OuterToolsTemplate/js/jquery.nicescroll.js" type="text/javascript"></script>
    <script src="/OuterToolsTemplate/assets/jquery-ui/jquery-ui-1.10.1.custom.min.js" type="text/javascript"></script>
    <script src="/OuterToolsTemplate/js/jquery.ui.touch-punch.min.js"></script>
    <script src="/OuterToolsTemplate/js/jquery.customSelect.min.js"></script>

    <script src="/OuterToolsTemplate/js/respond.min.js"></script>


    <!--common script for all pages-->
    <script src="/OuterToolsTemplate/js/common-scripts.js"></script>

    <script src="/OuterToolsTemplate/js/sliders.js" type="text/javascript"></script>

    <script type="text/javascript">

        $(document).ready(function () {

            $(function () {
                $('select.styled').customSelect();
            });
        });


    </script>

</body>
</html>
